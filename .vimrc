syntax enable

set number
set showcmd
set cursorline
filetype indent on
set wildmenu
set lazyredraw
set showmatch

set incsearch 
set hlsearch

set tabstop=4
set softtabstop=4
set expandtab
set shiftwidth=4

inoremap jk <esc>
set nocompatible

set backspace=indent,eol,start

execute pathogen#infect()
